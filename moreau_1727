			$(function () {
				var images = [{
					small : 'images/MOREAU1727_PAGE_06.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_07.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_08.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_09.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_10.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_11.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_12.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_13.jpg',
				},{
					small : 'images/MOREAU1727_PAGE_14.jpg',
				}];
			 
				var curImageIdx = 1,
					total = images.length;
				var wrapper = $('#image-gallery'),
					curSpan = wrapper.find('.current');
				var viewer = ImageViewer(wrapper.find('.image-container'));
			 
				//display total count
				wrapper.find('.total').html(total);
			 
				function showImage(){
					var imgObj = images[curImageIdx - 1];
					viewer.load(imgObj.small, imgObj.big);
					curSpan.html(curImageIdx);
					
					if (curImageIdx == 1) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page1').innerHTML;
					} else if (curImageIdx == 2) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page2').innerHTML;
					} else if (curImageIdx == 3) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page3').innerHTML;
					} else if (curImageIdx == 4) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page4').innerHTML;
					} else if (curImageIdx == 5) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page5').innerHTML;
					} else if (curImageIdx == 6) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page6').innerHTML;
					} else if (curImageIdx == 7) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page7').innerHTML;
					} else if (curImageIdx == 8) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page8').innerHTML;
					} else if (curImageIdx == 9) {
						document.getElementById("Transcription").innerHTML = document.getElementById('page9').innerHTML;
					} 
				}
			 
				wrapper.find('.next').click(function(){
					 curImageIdx++;
					if(curImageIdx > total) curImageIdx = 1;
					showImage();
				});
			 
				wrapper.find('.prev').click(function(){
					 curImageIdx--;
					if(curImageIdx < 0) curImageIdx = total;
					showImage();
				});
			 
				//initially show image
				showImage();
				
				
				
				
				
			});
			</script>
		
